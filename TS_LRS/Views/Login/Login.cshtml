<body>
    <section style="padding:20px">
        <div class="container">

            <div class="col-md-12 text-center">
                <h2 class="heading-section">User Login</h2>
            </div>

            <div class="row justify-content-center">
                <div class="col-md-10">
                    <div class="wrap d-md-flex">
                        <div class="img" style="text-align:center;padding:100px">
                            <img src="images/Telanganalogo.png" alt="telangana logo" style="width:100px;height:100px" />
                            <div>
                                <h2><span style="color:orange">Government of Telangana </span><br />Layout Regularisation Scheme 2020</h2>
                            </div>
                        </div>



                        <div class="login-wrap p-4 p-md-5" style="background-color:#00a6cc">
                            <div class="d-flex">
                                <div class="w-100">
                                </div>
                                <div class="w-100">
                                    <p class="social-media d-flex justify-content-end">
                                        <a href="#" class="social-icon d-flex align-items-center justify-content-center"><span class="fa fa-facebook"></span></a>
                                        <a href="#" class="social-icon d-flex align-items-center justify-content-center"><span class="fa fa-twitter"></span></a>
                                    </p>
                                </div>
                            </div>
                            <div>
                                @using (Html.BeginForm("Login", "Login", FormMethod.Post, new { @id = "Save_LoginForm" }))
                                {
                                    
                                    <div class="form-group mb-3">
                                        <label asp-for="USRNAME" >User Name</label>
                                        <input type="text" asp-for="USRNAME" class="form-control" name="USRNAME" placeholder="Username" required />
                                    </div>
                                    <div class="form-group mb-3">
                                        <label asp-for="PASSWORD">Password</label>
                                        <input type="password" asp-for="PASSWORD" class="form-control" name="PASSWORD" placeholder="PASSWORD" required />
                                    </div>
                                    <div class="form-group">
                                        <button type="submit" class="form-control btn btn-primary rounded submit px-3">Sign In</button>
                                    </div>

                                }
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row justify-content-center">

                <div class="row">
                    <div class="col-md-12">
                        <p class="text-center text-white pt-3">
                            Designed & Developed by : <a href="https://www.cgg.gov.in">
                                <img src="images/cgg_logo.png" height="30px">
                            </a>
                        </p>
                    </div>
                </div>
            </div>

        </div>
    </section>
    <script src="~/scripts/jquery.min.js"></script>
    <script src="~/scripts/popper.js"></script>
    <script src="~/scripts/main.js"></script>
    <script src="~/scripts/bootstrap.min.js"></script>
</body>
</html>

