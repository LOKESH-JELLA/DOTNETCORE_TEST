@model TS_LRS.Models.Login.LoginUser;

@{
    Layout = null;
}


<!doctype html>
<html lang="en">
<head>
    <title>User Login</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="~/logincss/css.css" rel="stylesheet" />
    <link href="~/logincss/font-awesome.min.css" rel="stylesheet" />
    <link href="~/logincss/style.css" rel="stylesheet" />
